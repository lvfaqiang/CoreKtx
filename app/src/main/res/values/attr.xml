<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CusToolBar">
        <attr name="leftImg" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="titleSize" format="integer" />
        <attr name="titleColor" format="color" />
        <attr name="rightText" format="string" />
        <attr name="rightTextSize" format="integer" />
        <attr name="rightTextColor" format="color" />
    </declare-styleable>


    <declare-styleable name="InputEditText">
        <attr name="android:textSize" />
        <attr name="editPaddingLeft" format="dimension" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:hint" />
        <attr name="android:singleLine" />
        <attr name="android:inputType" />
        <attr name="android:maxLength" />
    </declare-styleable>
</resources>